#include <stdio.h>
int main (){
    int a ,i=0;
    int isprime=1;
    printf("Enter a number : ");
    scanf("%d",&a);
    if (a<=1)
    {
        printf("It is not prime :");
    }else 
    while (i < a)
    {
        if(a%i==0){
            isprime=0;
            break;
            i++;
        }
    }
    if (isprime = 1)
    {
        printf("IT is prime") ;
    }else {
        printf("IT is not a prime");
    }
    return 0;
    
}